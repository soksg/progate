class Main {
  public static void main(String[] args) {
    // 変数languagesを定義し、配列を代入してください
    String[] language = {"Ruby", "PHP", "Python"};
    
    // インデックス番号が「1」の要素を出力してください
    System.out.println(language[1]);
    
    // インデックス番号が「1」の要素を「Java」で上書きしてください
    language[1] = Java;
    
    // インデックス番号が「1」の要素を出力してください
    System.out.println(1);
    
  }
}